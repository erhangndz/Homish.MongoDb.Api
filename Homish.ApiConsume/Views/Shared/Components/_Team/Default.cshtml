@using DtoLayer.DTOS.TeamDtos;
@model List<ResultTeamDto>

<section class="w3l-team" id="team">
    <div class="team-block py-5">
        <div class="container py-lg-5">
            <div class="title-content text-center mb-lg-3 mb-4">
                <h6 class="sub-title">Ekibimiz</h6>
                <h3 class="hny-title">Çalışanlarımızla Tanışın</h3>
            </div>
            <div class="row">
                @foreach (var item in Model)
                {
                    
                
                <div class="col-lg-4 col-md-6 mt-lg-5 mt-4">
                    <div class="box16">
                        <a href="#url"><img src="@item.Image" alt="" class="img-fluid" /></a>
                        <div class="box-content">
                            <h3 class="title"><a href="#url">@item.NameSurname</a></h3>
                            <span class="post">@item.JobTitle</span>
                            <ul class="social">
                                <li>
                                    <a href="@item.Instagram" class="instagram">
                                        <span class="fa fa-instagram"></span>
                                    </a>
                                </li>
                                <li>
                                    <a href="@item.Linkedin" class="linkedin">
                                        <span class="fa fa-linkedin"></span>
                                    </a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
                }
           
            </div>
        </div>
    </div>
</section>